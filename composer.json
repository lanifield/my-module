{
    "name": "lanifield/my-module",
    "description": "My SilverStripe vendor-module template.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "homepage": "http://github.com/lanifield/my-module",
    "readme": "http://github.com/lanifield/my-module/README.md",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Lani Field",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "http://github.com/lanifield/my-module/docs/en/userguide.md",
        "issues": "http://github.com/lanifield/my-module/issues"
    },
    "require": {
        "silverstripe/cms": "^4.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "LaniField\\MyModule\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "admin",
            "client"
        ]
    }
}
